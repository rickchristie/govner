      "type": "shell",
      "command": "${workspaceFolder}/sandb/proxy/start.sh",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "presentation": {
        "clear": false,
        "close": true,
        "echo": true,
        "focus": false,
        "panel": "dedicated",
