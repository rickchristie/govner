      },
      "presentation": {
        "clear": false,
        "echo": true,
        "focus": false,
        "panel": "dedicated",
        "reveal": "always",
        "showReuseMessage": false
      },
      "problemMatcher": [],
      "isBackground": false
    },
    {
      "label": "AI: Shell",
      "type": "shell",
      "command": "${workspaceFolder}/sandb/shell.sh",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "presentation": {
        "clear": true,
        "echo": true,
        "focus": true,
        "panel": "new",
        "group": "ai-shell",
        "close": true,
        "reveal": "always",
        "showReuseMessage": false
      },
      "runOptions": {
        "instanceLimit": 99,
        "reevaluateOnRerun": true
      },
      "problemMatcher": [],
      "isBackground": true
    },
    {
      "label": "AI: Doctor",
      "type": "shell",
      "command": "${workspaceFolder}/sandb/shell.sh -c '${workspaceFolder}/sandb/doctor.sh'",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "presentation": {
        "clear": true,
        "echo": true,
        "focus": true,
        "panel": "dedicated",
        "reveal": "always",
        "showReuseMessage": false
      },
      "problemMatcher": [],
      "isBackground": false
    },
    {
      "label": "pgflock: Build",
      "type": "shell",
      "command": "go build -o ${workspaceFolder}/pgflock/pgflock ${workspaceFolder}/pgflock && notify-send 'pgflock' 'Build complete' || { notify-send 'pgflock' 'Build failed'; read -p 'Press Enter to close...'; exit 1; }",
      "options": {
        "cwd": "${workspaceFolder}/pgflock"
      },
      "presentation": {
        "clear": true,
        "echo": true,
        "focus": false,
        "panel": "dedicated",
        "reveal": "silent",
        "close": true,
        "showReuseMessage": false
      },
      "problemMatcher": "$go",
      "isBackground": false
    },
    {
      "label": "gowt: Test Current Package",
      "type": "shell",
      "command": "go clean -testcache && gowt .",
      "options": {
        "cwd": "${fileDirname}"
      },
      "presentation": {
        "clear": true,
        "close": true,
        "echo": true,
        "focus": true,
        "panel": "dedicated",
        "group": "test",
        "reveal": "always",
        "showReuseMessage": false
      },
      "problemMatcher": "$go",
      "isBackground": false
    },
    {
      "label": "gowt: Test All pgflock",
      "type": "shell",
      "command": "go clean -testcache && gowt ./...",
      "options": {
        "cwd": "${workspaceFolder}/pgflock"
      },
      "presentation": {
        "clear": true,
        "close": true,
        "echo": true,
        "focus": true,
