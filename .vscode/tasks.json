{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "AI: Build Proxy",
      "type": "shell",
      "command": "${workspaceFolder}/sandb/proxy/build.sh",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "presentation": {
        "clear": false,
        "echo": true,
        "focus": false,
        "panel": "dedicated",
        "reveal": "always",
        "showReuseMessage": false
      },
      "problemMatcher": [],
      "isBackground": false
    },
    {
      "label": "AI: Build CLI",
      "type": "shell",
